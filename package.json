{
  "name": "easygsm",
  "version": "3.0.1",
  "description": "EasyGSM is a tool used to make downloading, commissioning, managing, and decommissioning easy and automated",
  "author": {
    "name": "Matthew Manka",
    "email": "mrmanka0307@gmail.com"
  },
  "license": "LICENSE.md",
  "main": "./out/src/frontend/.js/main.js",
  "scripts": {
    "test": "jest",
    "build": "npm run webpack && electron-builder",
    "electron": "npm run webpack && electron . --trace-warnings",
    "webpack": "webpack --mode production",
    "release:major": "npm version major && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:patch": "npm version patch && git push && git push --tags"
  },
  "build": {
    "appId": "com.mrmanka.easygsm",
    "productName": "EasyGSM",
    "extraResources": [
      "external-dependencies/**/*"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "src/frontend/images/EasyGSMLogoIcon.png",
      "packageCategory": "Utility",
      "desktop": {
        "Name": "EasyGSM",
        "Icon": "src/frontend/images/EasyGSMLogoIcon.png",
        "Terminal": "false",
        "Type": "Application",
        "Categories": "Utility"
      },
      "maintainer": "Matthew Manka"
    },
    "icon": "src/frontend/images/EasyGSMLogoIcon.png",
    "files": [
      "out/**/*"
    ],
    "nsis": {
      "license": "./license.md",
      "oneClick": false,
      "perMachine": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "src/frontend/images/EasyGSMLogo.ico",
      "installerHeader": "src/frontend/images/EasyGSMLogo.bmp",
      "installerSidebar": "src/frontend/images/EasyGSMLogo.bmp",
      "uninstallerIcon": "src/frontend/images/EasyGSMLogo.ico",
      "uninstallerSidebar": "src/frontend/images/EasyGSMLogo.bmp",
      "deleteAppDataOnUninstall": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@sentry/webpack-plugin": "^2.13.0",
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.13",
    "@types/node-cron": "^3.0.11",
    "@types/react-transition-group": "^4.4.10",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@sentry/browser": "^7.99.0",
    "@sentry/react": "^7.103.0",
    "@sentry/tracing": "^7.103.0",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "archiver": "^6.0.1",
    "electron-log": "^5.1.1",
    "electron-store": "^8.1.0",
    "fs": "^0.0.1-security",
    "node-cron": "^3.0.3",
    "os": "^0.1.2",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.2",
    "react-transition-group": "^4.4.5",
    "sudo-prompt": "^9.2.1",
    "tree-kill": "^1.2.2"
  },
  "pkg": "./pkg-config.json"
}
