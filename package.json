{
  "name": "palserver-manager",
  "version": "1.0.11",
  "description": "A Palworld server manager to auto manage your server",
  "main": "./out/src/ui/main.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --outDir ./out && pkg -t node18-win-x64 ./out/src/data/index.js -o ./FilterLogs.exe && copyfiles -u 0 src/ui/html/* out/ && copyfiles -u 0 src/ui/css/* out/ && copyfiles -u 0 src/ui/images/* out/",
    "build:react": "webpack --mode production",
    "start:poc": "npm run build && electron ./out/src/ui/main.js",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:patch": "npm version patch && git push && git push --tags"
  },
  "author": "Matthew Manka",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.13",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "electron-builder": "^24.9.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "electron": "^28.2.0",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0"
  },
  "pkg": "./pkg-config.json"
}
