{
    "editor.formatOnSave": true,
    "tabWidth": 2,
    "printWidth": 180,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "preserve",
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "ignore",
    "singleAttributePerLine": false
}
